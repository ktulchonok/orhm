<%= simple_form_for(@ticket) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :enter_adult %>
    <%= f.input :enter_child %>
    <%= f.input :excursion_adult %>
    <%= f.input :excursion_child %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
